<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="670.0" prefWidth="972.0" stylesheets="@Styles.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <AnchorPane id="AnchorPane" fx:id="anchorpane_center_main" styleClass="anchor-pane">
         <children>
            <ImageView fx:id="imageview_video" fitHeight="418.0" fitWidth="657.0" layoutX="49.0" layoutY="91.0" pickOnBounds="true" preserveRatio="true" />
         </children>
      </AnchorPane>
   </center>
   <left>
      <VBox spacing="20.0" BorderPane.alignment="CENTER">
         <children>
            <Label styleClass="text-header" text="CAR COUNT" />
            <TextField fx:id="textfield_carcount" editable="false" prefHeight="39.0" prefWidth="154.0" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </left>
   <bottom>
      <HBox alignment="CENTER_LEFT" prefHeight="71.0" prefWidth="972.0" spacing="20.0" styleClass="lower-dock" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="button_choosefile" mnemonicParsing="false" onAction="#button_choosefileOnClick" prefHeight="49.0" prefWidth="159.0" text="CHOOSE FILE">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Button>
            <TextField fx:id="textfield_path" editable="false" prefHeight="38.0" prefWidth="299.0" />
            <Region HBox.hgrow="ALWAYS" />
            <JFXButton fx:id="jfxbutton_play" alignment="CENTER" buttonType="RAISED" contentDisplay="CENTER" onAction="#jfxbutton_playOnClick" prefHeight="40.0" prefWidth="121.0" ripplerFill="WHITE" style="-fx-background-color: #f27920; -fx-font-weight: bold;" text="PLAY" textFill="WHITE" />
            <Button fx:id="button_playpause" mnemonicParsing="false" onAction="#button_playpauseOnClick" prefHeight="40.0" prefWidth="88.0" text="PLAY/PAUSE">
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </bottom>
</BorderPane>
